<?xml version="1.0" encoding="utf-8" ?>
<widget id="MarkJS.widget.MarkJS" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mendix.com/widget/1.0/ widget.xsd">
    <name>MarkJS</name>
    <description>A text highlighter</description>
    <icon></icon>
    <properties>
        <property key="inputAttribute" type="attribute" required="true"> 
            <caption>Search attribute</caption>
            <category>General</category>
            <description>The attribute to use as search parameter</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>       

        <property key="accuracy" type="enumeration" defaultValue="partially">
            <caption>Accuracy</caption>
            <category>General</category>
            <description>How to match text based on the searchstring.</description>
            <enumerationValues>
                <enumerationValue key="partially">Partially</enumerationValue>
                <enumerationValue key="exactly">Exactly</enumerationValue>
                <enumerationValue key="complementary">Complementary</enumerationValue>
            </enumerationValues>
        </property>
        
        <property key="separateWordSearch" type="boolean" defaultValue="true">
            <caption>Separate word search</caption>
            <category>General</category>
            <description>If true, searches each word in the search string individually.</description>
        </property>        

        <property key="caseSensitive" type="boolean" defaultValue="false">
            <caption>Case sensitive</caption>
            <category>General</category>
            <description>If true, search is case sensitive.</description>
        </property>        

        <property key="diacritics" type="boolean" defaultValue="true">
            <caption>Ignore diacritics</caption>
            <category>General</category>
            <description>If true, doner matches d√∂ner</description>
        </property>

        <property key="wildcards" type="enumeration" defaultValue="enabled">
            <caption>Use wildcards</caption>
            <category>General</category>
            <description>Disabled: Disable wildcard usage
Enabled: match non-space characters
With spaces: match space and non-space characters
? will match 0 or 1 character, * will match 0 or more characters.</description>
            <enumerationValues>
                <enumerationValue key="enabled">Enabled</enumerationValue>
                <enumerationValue key="withSpaces">With Spaces</enumerationValue>
                <enumerationValue key="disabled">Disabled</enumerationValue>
            </enumerationValues>
        </property>

        <property key="color" type="enumeration" defaultValue="yellow">
            <caption>Highlight color</caption>
            <category>General</category>
            <description>The color used to highlight matches</description>
            <enumerationValues>
                <enumerationValue key="yellow">Yellow</enumerationValue>
                <enumerationValue key="red">Red</enumerationValue>
                <enumerationValue key="blue">Blue</enumerationValue>
                <enumerationValue key="aqua">Aqua</enumerationValue>
                <enumerationValue key="fuchsia">Fuchsia</enumerationValue>
                <enumerationValue key="other">Other</enumerationValue>
            </enumerationValues>
        </property>

        <property key="customColor" type="string" defaultValue="" required="false">
            <caption>Custom color</caption>
            <category>General</category>
            <description>If highlight color 'Other', use this CSS color.</description>
        </property>

        <property key="element" type="string" defaultValue="mark">
            <caption>Inserted element</caption>
            <category>Advanced</category>
            <description>This tag is inserted around all hits. N.b.: choose something that does not affect the styling of your page.</description>
        </property>

        <property key="context" type="string" defaultValue="body" >
            <caption>Search context</caption>
            <category>Advanced</category>
            <description>A css selector (jQuery) to select the html element where marking is applied.</description>
        </property>


    </properties>
</widget>
